{
  "version": 2,
  "builds": [
    { "src": "symione/package.json", "use": "@vercel/static-build", "config": { "distDir": "symione/build" } }
  ],
  "installCommand": "cd symione && npm ci",
  "buildCommand": "cd symione && npm run build",
  "routes": [
    { "src": "/api/(.*)", "dest": "https://api.symione.com/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}

{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://api.symione.com/api/:path*" },
    { "source": "/((?!api/).*)", "destination": "/symione/index.html" }
  ]
}