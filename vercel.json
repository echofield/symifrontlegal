{
  "version": 2,
  "framework": "vite",
  "installCommand": "npm --prefix symione ci",
  "buildCommand": "npm --prefix symione run build",
  "outputDirectory": "symione/build",
  "rewrites": [
    { "source": "/api/:path*", "destination": "https://api.symione.com/api/:path*" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}


