{
  "version": 2,
  "buildCommand": "echo 'Static build complete'",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://symilegalback.vercel.app",
    "NEXT_PUBLIC_GOOGLE_MAPS_API_KEY": "YOUR_KEY_HERE"
  }
}
