{
  "version": 2,
  "installCommand": "npm ci --no-audit --progress=false",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "https://symilegalback-virid.vercel.app/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}


